#test correct federation upstream with default value
rabbit_setup:
    connections:
        default:
    vhosts:
        "/":
            connection: default
            parameters:
                federation-upstream:
                    thor:
                        uri: "amqp://thor"
                        expires: ""
                        message-ttl: ""
--EXPECT--
[
    "connections" => [
        "default" => [
            "user" => "guest",
            "password" => "guest",
            "host" => "127.0.0.1",
            "port" => 15672,
        ]
    ],
    "vhosts" => [
        "/" => [
            "connection" => "default",
            "parameters" => [
                "federation-upstream" => [
                    "thor" => [
                        "uri" => "amqp://thor",
                        "expires" => null,
                        "message-ttl" => null,
                        "max-hops" => 1,
                        "prefetch-count" => 1000,
                        "reconnect-delay" => 5,
                        "ack-mode" => "on-confirm",
                        "trust-user-id" => false,
                    ]
                ],
                "federation-upstream-set" => []
            ],
            "policies" => [],
            "exchanges" => [],
            "queues" => [],
        ]
    ]
]
